<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="490.0" prefWidth="460.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mediaplayer.MainController">
    <VBox layoutY="59.0" prefHeight="431.0" prefWidth="460.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">

        <children>
            <TableView fx:id="table" prefHeight="333.0" prefWidth="460.0">
                <columns>
                    <TableColumn fx:id="trackName" />
                </columns>
            </TableView>
            <HBox>
                <Button onAction="#onAdd" text="add" />
                <Button onAction="#onPlay" text="play" />
                <Button onAction="#onPause" text="pause" />
                <Button onAction="#onStop" text="stop" />
                <Button onAction="#onPlaylists" text="playlists" />

                <Button onAction="#onEqualizer" prefHeight="26.0" prefWidth="67.0" text="equalizer" />
            </HBox>
         <BorderPane fx:id="wavePane" prefHeight="33.0" prefWidth="460.0" />
            <AnchorPane prefHeight="39.0" prefWidth="460.0">
            <children>
               <Slider fx:id="progressSlider" max="5000.0" prefHeight="39.0" prefWidth="460.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
            </children></AnchorPane>
        </children>

    </VBox>
</AnchorPane>
